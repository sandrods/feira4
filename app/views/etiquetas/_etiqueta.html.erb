<%= content_tag_for(:tr, etiqueta) do %>
  <td><%= etiqueta.cor.nome %></td>
  <td><%= etiqueta.tamanho.nome %></td>
  <td style="width: 30px;">
    <%= link_to icon(:trash), 
                [@produto, etiqueta], 
                remote: true, 
                class: 'link_delete',
                method: :delete %>
  </td>
<% end -%>